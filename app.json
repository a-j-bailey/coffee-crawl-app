{
  "expo": {
    "name": "Coffee Crawl",
    "slug": "coffee-crawl-tulsa",
    "scheme": "coffee-crawl",
    "description": "Enjoy some great coffee!",
    "primaryColor": "#CB0C9F",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "version": "0.1.2",
    "splash": {
      "image": "./assets/splash_screen.png",
      "resizeMode": "cover",
      "backgroundColor": "#f5f5f5"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      // "buildNumber": "3",
      "bundleIdentifier": "com.magicmirrorcreative.coffee-crawl",
      "infoPlist": {
        "NSCameraUsageDescription": "Access to your camera will allow you to scan and upload a card for payment. This is the only thing we use this access for."
      },
      "entitlements": {
        "com.apple.developer.in-app-payments": ["merchant.com.coffee-crawl"]
      }
    },
    "android": {
      "package": "com.magicmirrorcreative.coffeecrawltulsa",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#CB0C9F"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "f496207d-8f07-4e0f-b5ab-4c8f523f19db"
      }
    }
  }
}
